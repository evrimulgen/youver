{
  "parser": "babel-eslint",
  "extends": ["standard", "standard-react"],
  "plugins": ["babel"],
  "env": {
    "browser": true
  },
  "rules": {
    "quotes": ["error", "double"],
    "semi": [2, "never"],
    "jsx-quotes": [0, "prefer-double"],
    "react/prop-types": "off",
    "space-before-function-paren": "off",
    "comma-dangle": ["error", "always-multiline"]
  }
}
